{
  "name": "shamimshams/lw-datatable",
  "description": "A Laravel Livewire Datatable Helper Package",
  "license": "MIT",
  "keywords": [
    "datatable",
    "helper"
  ],
  "type": "library",
  "authors": [
    {
      "name": "shamim shams",
      "email": "shamim.idb@hotmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "illuminate/support": "^8.0|^9.0",
    "livewire/livewire": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "ShamimShams\\LWDatatable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ShamimShams\\LWDatatable\\LWDataTableServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
